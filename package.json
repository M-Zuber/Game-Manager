{
  "name": "Game-Manager",
  "version": "0.0.0",
  "description": "A small tool for managing games played for money",
  "main": "app.js",
  "watch": {
    "test": "*.js"
  },
  "scripts": {
    "test": "mocha tests/*.test.coffee tests/**/*.test.coffee --compilers coffee:coffee-script --reporter spec --watch",
    "watch": "npm-watch",
    "compile": "coffee -cw ./src/*.coffee ./tests/*.coffee"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/M-Zuber/Game-Manager.git"
  },
  "author": "M-Zuber",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/M-Zuber/Game-Manager/issues"
  },
  "homepage": "https://github.com/M-Zuber/Game-Manager",
  "devDependencies": {
    "chai": "^1.10.0",
    "mocha": "^2.0.1",
    "npm-watch": "0.0.0"
  }
}
